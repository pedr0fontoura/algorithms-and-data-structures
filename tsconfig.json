{
  "compilerOptions": {
      "outDir": "dist",
      "allowJs": false,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "resolveJsonModule": true,
      "noImplicitReturns": true,
      "esModuleInterop": true,
      "target": "es6",
      "module": "commonjs",
  },
  "include": [
      "./src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
}